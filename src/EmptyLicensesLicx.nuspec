<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata minClientVersion="3.3">
    <id>EmptyLicensesLicx</id>
    <version>0.0.1-local</version>
    <title>Empty Licenses.licx for .NET projects</title>
    <authors>C. Augusto Proiete</authors>
    <owners>augustoproiete</owners>
    <iconUrl>https://raw.github.com/augustoproiete/EmptyLicensesLicx/master/img/empty-licenses-licx.png</iconUrl>
    <releaseNotes>https://github.com/augustoproiete/EmptyLicensesLicx/releases/tag/v$version$</releaseNotes>
    <license type="file">LICENSE</license>
    <repository type="git" url="https://github.com/augustoproiete/EmptyLicensesLicx" />
    <projectUrl>https://github.com/augustoproiete/EmptyLicensesLicx</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>An easy approach to resolve issues with compiling the licenses.licx file.</summary>
    <description>
      Hooks in the build pipeline to make sure that the `Properties\licenses.licx` file* is always empty before the compiler runs.
      * (or `My Project\licenses.licx` for VB .NET projects)
    </description>
    <copyright>Copyright (c) C. Augusto Proiete 2016-2020</copyright>
    <language>en-US</language>
    <tags>licenses licx Licenses.licx CI MSBuild Telerik DevExpress Infragistics</tags>
    <developmentDependency>true</developmentDependency>
  </metadata>
  <files>
    <file src="Readme.txt" />
    <file src="EmptyLicensesLicx.targets" target="build" />
    <file src="..\LICENSE" target="\LICENSE" />
  </files>
</package>
